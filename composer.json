{
    "name": "ajthenewguy/php8-api-server",
    "description": "A ReactPHP-based API server framework.",
    "type": "library",
    "require": {
        "react/event-loop": "^1.2",
        "react/http": "^1.5",
        "react/stream": "^1.2",
        "react/promise": "^2.8",
        "vlucas/phpdotenv": "5.3.x-dev",
        "nesbot/carbon": "^2.51",
        "clue/stdio-react": "^2.4",
        "firebase/php-jwt": "^5.4",
        "clue/reactphp-sqlite": "^1.1",
        "react/mysql": "^0.5.5",
        "swiftmailer/swiftmailer": "^6.2",
        "react/child-process": "^0.6.3",
        "react/cache": "^1.1",
        "jenssegers/blade": "^1.4",
        "wyrihaximus/react-http-middleware-session": "^4.1",
        "mockery/mockery": "^1.4"
    },
    "require-dev": {
        "phpunit/phpunit": "10.0.x-dev",
        "phpstan/phpstan": "0.12.x-dev"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Ajthenewguy\\Php8ApiServer\\": "src/",
            "Tests\\": "tests/"
        },
        "files": ["src/functions.php"]
    },
    "authors": [
        {
            "name": "Allen McCabe",
            "email": "allenmccabe@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true
}
